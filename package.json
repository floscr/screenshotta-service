{
  "name": "screenshotta-service",
  "version": "0.0.1",
  "description": "Screenshot service for Screenshotta app",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "cross-env BABEL_ENV=commonjs mocha 'src/**/*.spec.js' --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "cross-env BABEL_ENV=commonjs istanbul cover _mocha -- 'src/**/*.spec.js' --compilers js:babel-register --recursive",
    "lint": "eslint src",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "electron",
    "screenshoot"
  ],
  "author": "Florian Schr√∂dl <hello@florianschroedl.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "cross-env": "^3.0.0",
    "eslint": "^3.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "expect-to-be-a-promise": "^1.0.2",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "electron": "^11.5.0",
    "eslint-config-standard": "^10.2.1"
  }
}
