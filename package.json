{
  "name": "screenshotta-service",
  "version": "0.0.1",
  "description": "Screenshot service for Screenshotta app",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:examples": "nodemon buildScripts/runExample.js",
    "cover": "cross-env BABEL_ENV=commonjs istanbul cover _mocha -- 'src/**/*.spec.js' --compilers js:babel-register --recursive",
    "lint": "eslint src",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "electron",
    "screenshoot"
  ],
  "author": "Florian Schr√∂dl <hello@florianschroedl.com>",
  "license": "MIT",
  "dependencies": {
    "electron": "^1.6.5"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "cross-env": "^3.0.0",
    "devtron": "^1.4.0",
    "electron-debug": "^1.1.0",
    "eslint": "^3.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "expect-to-be-a-promise": "^1.0.2",
    "istanbul": "^1.0.0-alpha",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "node-loader": "^0.6.0",
    "nodemon": "^1.11.0",
    "regenerator-runtime": "^0.10.3",
    "rimraf": "^2.5.4",
    "tree-kill": "^1.1.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4"
  }
}
